---
import { render } from "astro:content";
import { getTranslatedEntry } from "../i18n/locale";
import Shell from "../layout/Shell.astro";

const dataProtection = await getTranslatedEntry(
  Astro.currentLocale!,
  "data-protection"
);
const { Content } = await render(dataProtection);
---

<Shell container>
  <span class="content">
    <Content />
  </span>
</Shell>
